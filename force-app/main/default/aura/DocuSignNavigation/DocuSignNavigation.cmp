<aura:component implements="lightning:availableForFlowScreens" controller="DocuSignNavigationController">

    <aura:attribute name="sourceURL" type="String"/>
    <aura:attribute name="targetURL" type="String"/>
    <aura:attribute name="pageReference" type="Object"/>
    <lightning:workspaceAPI aura:id="workspace"/>
    
    <lightning:navigation aura:id="navService"/>

    <aura:handler name="init" value="{!this}" action="{!c.init}"/>

    <div class="slds-align_absolute-center" style="height:5rem">
       <lightning:button variant="brand" label="Go To DocuSign" title="Go To DocuSign" onclick="{! c.handleClick }"/>
    </div>

</aura:component>