    <start>
        <locationX>210</locationX>
        <locationY>0</locationY>
        <connector>
            <targetReference>AssignedToAM</targetReference>
        </connector>
        <filterFormula>AND(
{!$Record.RecordType.DeveloperName} = &apos;Handover&apos;,
OR(
AND(
ISNEW(),
!ISBLANK({!$Record.Sales_Manager__c})),

AND(
!ISBLANK({!$Record.Sales_Manager__c}),
ISCHANGED({!$Record.Sales_Manager__c})),

ISCHANGED({!$Record.Assigned_AM__c}),
ISCHANGED({!$Record.IM__c}),
ISCHANGED({!$Record.AssignedPom__c}),
