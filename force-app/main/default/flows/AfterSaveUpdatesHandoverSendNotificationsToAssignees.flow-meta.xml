    <decisions>
        <description>Checks whether the Opportunity is already on Closed Won and therefore whether the Assigned AM can become the Account Owner</description>
        <name>AccountOwnershipUpdated</name>
        <label>Can Account Ownership be updated?</label>
        <locationX>182</locationX>
        <locationY>539</locationY>
        <defaultConnector>
            <targetReference>AMPoolisAssigned</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>YesAccountOwnershipUpdated</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Opportunity__r.StageName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Transacting</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Account__r.OwnerId</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record.Assigned_AM__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>UpdateAccountOwnership</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
