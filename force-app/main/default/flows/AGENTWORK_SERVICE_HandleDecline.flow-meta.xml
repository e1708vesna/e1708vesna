        <locationX>182</locationX>
        <locationY>539</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>UpdateCase</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.UserId</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <elementReference>GetCaseRecord.OwnerId</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>GetCaseRecord.PreviousQueueID__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>UpdateCaseOwner</targetReference>
            </connector>
            <label>Update Case</label>
        </rules>
    </decisions>
