    <decisions>
        <name>DetermineSecondaryPriority</name>
        <label>Determine Secondary Priority</label>
        <locationX>842</locationX>
        <locationY>287</locationY>
        <defaultConnector>
            <targetReference>UpdateToEmpty</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>StatusUrgent</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Priority</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Urgent</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>UpdatetoUrgent</targetReference>
            </connector>
            <label>Status Urgent?</label>
        </rules>
        <rules>
            <name>CaseEscalated</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.IsEscalated</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>UpdatetoEscalated</targetReference>
            </connector>
            <label>Case Escalated?</label>
        </rules>
        <rules>
            <name>HighPriority</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Priority</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>High</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>UpdatetoHighPriority</targetReference>
            </connector>
            <label>High Priority?</label>
        </rules>
        <rules>
            <name>VIPAcccount</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Account.ServiceLevel__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>VIP</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>UpdatetoVIP</targetReference>
            </connector>
            <label>VIP Acccount?</label>
        </rules>
        <rules>
            <name>HandoverReasonHandoverToNextRegion</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.HandoverReason__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Handover To Next Region</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>UpdateToNextRegion</targetReference>
            </connector>
            <label>Handover Reason &apos;Handover To Next Region&apos;</label>
        </rules>
        <rules>
            <name>HandoverReasonOther</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.HandoverReason__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.HandoverReason__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Handover To Next Region</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>UpdatetoHandoverReasonOther</targetReference>
            </connector>
            <label>Handover Reason Other</label>
        </rules>
    </decisions>
