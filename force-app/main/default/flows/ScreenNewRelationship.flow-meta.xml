    <environments>Default</environments>
    <formulas>
        <name>forIsPartner</name>
        <dataType>Boolean</dataType>
        <expression>IF( OR({!Get_Account.RecordType.DeveloperName} = &apos;PartnerAccountv2&apos;, AND({!Get_Account.RecordType.DeveloperName} = &apos;Business_Account&apos;, {!Get_Account.NumberPartnerTypes__c} &gt; 0)), true, false)</expression>
    </formulas>
    <formulas>
        <description>Key prefix of the record Id object</description>
        <name>forKeyPrefix</name>
        <dataType>String</dataType>
        <expression>LEFT({!recordId},3)</expression>
    </formulas>
    <formulas>
        <name>forObject</name>
        <dataType>String</dataType>
        <expression>IF(BEGINS({!recordId},&quot;00Q&quot;), &quot;Lead&quot;,
IF(BEGINS({!recordId},&quot;001&quot;), &quot;Account&quot;,
IF(BEGINS({!recordId},&quot;006&quot;), &quot;Opportunity&quot;,
IF(BEGINS({!recordId},&quot;a3w&quot;), &quot;PartnerType&quot;,
IF(BEGINS({!recordId},&quot;a3y&quot;), &quot;PartnerType&quot;,
&quot;Other&quot;)))))</expression>
    </formulas>
